@model IndexView


<h1>@TempData["success"] </h1>


<h2> Login </h2>



<form asp-action="Login" asp-controller="Home" method="post">
    <div class="form-group">
        <span asp-validation-for="LogUser.log_email"></span>
        <label asp-for="LogUser.log_email"></label>
        <input asp-for="LogUser.log_email">
    </div>
    <div class="form-group">
        <span asp-validation-for="LogUser.log_password"></span>
        <label asp-for="LogUser.log_password"></label>
        <input asp-for="LogUser.log_password">
    </div>

    <input type="submit" value="Login">
</form>